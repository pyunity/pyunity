"""
Functions to get information about the PyUnity's installation,
distribution or build. The main function is :func:`printInfo`.

"""

from typing import TYPE_CHECKING, Dict, List, Tuple, Callable, IO, Union, Any
from importlib.metadata import Distribution

if TYPE_CHECKING:
    _Requirements = Dict[str, List[str]]
    _VersionFunc = Callable[[str], str]

def getRepoInfo() -> None: ...
def getTomlLoader() -> Tuple[Callable[[Union[IO[str], IO[bytes]]], Dict[str, Any]], str]: ...
def formatName(item: str) -> str: ...
def getReqsFromToml(version: _VersionFunc) -> _Requirements: ...
def getReqsFromRepo(path: str, version: _VersionFunc) -> _Requirements: ...
def getReqsFromDistribution(dist: Distribution, version: _VersionFunc, gitInfo: bool = ...) -> _Requirements: ...
def printSystemInfo() -> None: ...
def getReqs() -> _Requirements: ...
def printReqs(reqs: _Requirements) -> None: ...
def printInfo() -> None: ...
